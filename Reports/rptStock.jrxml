	<queryString>
		<![CDATA[SELECT
     tblstock.`id` AS tblstock_id,
     tblstock.`itemname` AS tblstock_itemname,
     tblstock.`total_qty` AS tblstock_total_qty,
     tblitem.`id` AS tblitem_id,
     tblitem.`grrno` AS tblitem_grrno,
     tblitem.`itemname` AS tblitem_itemname,
     tblitem.`description` AS tblitem_description,
     tblitem.`rate` AS tblitem_rate,
     tblitem.`hsn` AS tblitem_hsn,
     tblitem.`dept` AS tblitem_dept,
     tblitem.`min_order_level` AS tblitem_min_order_level,
     tblitem.`max_order_level` AS tblitem_max_order_level,
     tblitem.`uom` AS tblitem_uom,
     tblitem.`tax` AS tblitem_tax,
     tblitem.`rack` AS tblitem_rack
FROM
     `tblstock` tblstock,
     `tblitem` tblitem
WHERE `tblstock`.itemname=`tblitem`.itemname]]>
	</queryString>
