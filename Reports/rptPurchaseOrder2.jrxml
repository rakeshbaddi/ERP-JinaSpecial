     tblitem.`maker` AS tblitem_maker,
     tblitem.`uom` AS tblitem_uom,
     tblitem.`tax` AS tblitem_tax
FROM
         tblpurchaseorder,
         tblpurchaseorderdetails,
         tblitem
where
      `tblpurchaseorder`.pono=$P{pono} and
       `tblpurchaseorderdetails`.pono=$P{pono} and
      `tblpurchaseorderdetails`.itemname=`tblitem`.itemname]]>
