	<queryString language="SQL">
		<![CDATA[SELECT
     tblpurchaseorder.`id` AS tblpurchaseorder_id,
     tblpurchaseorder.`pono` AS tblpurchaseorder_pono,
     tblpurchaseorder.`refno` AS tblpurchaseorder_refno,
